struct InnerInnerStruct {
    field: i64
}

struct InnerStruct {
    inner_inner: InnerInnerStruct
}

struct OuterStruct {
    inner: InnerStruct
}

fn extract_field(outer: OuterStruct) i64 {
    outer.inner.inner_inner.field
}

fn main() i64 {
    let outer = OuterStruct { 
        inner: InnerStruct {
            inner_inner: InnerInnerStruct {
                field: 42,
            },
        },
    };

    extract_field(outer)
}
